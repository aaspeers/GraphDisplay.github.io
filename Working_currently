        // Populate dropdown

        let chart; // Declare chart in the outer scope so it can be accessed globally\

        $(function () {
            var chart;
            $(document).ready(function () {
                // $.getJSON("data.php", function(json) {

                chart = new Highcharts.Chart({ 
                    chart: {
                        backgroundColor: '#000000',
                        renderTo: 'container',
                        type: 'line',
                        marginRight: 130,
                        marginBottom: 60,
                        height: 475,
                        width: 800,
                        color: '#123241',
                        
                    },
                    title: {
                        text: 'Plot of n & k Values of SbS vs. Wavelength (nm)',
                        
                        style: {
                            color: "white ",
                            fontFamily: "Barlow",
                            fontSize: "25px",

                        }

                    },
                    subtitle: {
                        text: '',
                        x: -20,
                        style: {
                            color: '#b0abab',
                        }
                    },

                    colors: {
                        color: 'green'
                    },

                    //plotOptions: {
                    //    series: {
                    //        color: '#FF0000'
                    //    }
                    xAxis: {
                        lineColor: '#E6E6E6',
                        lineWidth: 1,
                        tickColor: '#E6E6E6',
                        rangeDescription: 'Range: 300 to 1700',
                        showLastLabel: true,
                        title: {
                            enabled: true,
                            text: 'Wavelength (nm)',
                            style: {
                                color: '#b0abab',
                            }

                        },
                        labels: {
                            format: '{value}'
                            , style: {
                                color: '#b0abab',
                            }
                        },




                    },
                    exporting: {
                        buttons: {
                            contextButton: {
                                buttonSpacing: 10,
                                symbolStrokeWidth: 2,
                                symbolStroke: '#b0abab',
                                backgroundColor: '#000000',
                                theme: {
                                    fill: '#393939'
                                }
                            }
                        }
                    },
                    plotOptions: {
                        series: {
                            marker: {
                                enabled: false
                            }
                        }
                    },
                    yAxis: {
                        rangeDescription: 'Range: 0 to 5',
                        showLastLabel: true,
                        lineColor: '#6E6E6E',
                        lineWidth: 1,
                        gridLineDashStyle: 'dot',
                        title: {
                            enabled: true,
                            text: 'n , k value'
                            , style: {
                                color: '#b0abab',
                            }
                        },

                        labels: {
                            format: '{value}'
                            , style: {
                                color: '#b0abab',
                            }
                        },
                    },

                    credits: {
                        enabled: false
                    },

                    tooltip: {
                        formatter: function () {
                            return '<b>' + this.series.name + '</b><br/>' +
                                this.x + ': ' + this.y;
                        }

                    },
                    navigation: {

                        menuStyle: {
                            background: '#E0E0E0',
                        }
                    },
                    legend: {
                        layout: 'vertical',
                        align: 'right',
                        verticalAlign: 'top',
                        x: -10,
                        y: 100,
                        itemStyle: {
                            color: '#FFFFFF',
                            fontWeight: 'bold'
                        }
                    },


                    series: [...Standards.Si, ...Standards.Si3N4,...Standards.Au,...Standards.Ag,...Standards.Al,...materialData.SbS,], // Default material series - change when adding additional


                });

                chart.series[0].hide();
                chart.series[1].hide();
                chart.series[2].hide();
                chart.series[3].hide();
                chart.series[4].hide();
                chart.series[5].hide();
                chart.series[6].hide();
                chart.series[7].hide();
                chart.series[8].hide();
                chart.series[9].hide();
                // could potentially add a new set of series into change function - I think i need a new element ID for the standard change values - Possibly hust add another series that is permanenet and can be turned on or off.
                document.getElementById('materialDropdown').addEventListener('change', function () {
                    const selectedMaterial = this.value; // Get the selected value from the dropdown

                    // Ensure materialData[selectedMaterial] exists
                    if (materialData[selectedMaterial]) {
                        const newSeries = materialData[selectedMaterial]; // New series data


                        // Update the chart title
                        chart.setTitle({ text: `Plot of n & k Values of ${selectedMaterial} vs. Wavelength (nm)` }); // Update title dynamically

                        // Update each series individually
                        const seriesToRemove = ['n-SbS', 'k-SbS', 'n-SbSe', 'k-SbSe', 'n-GSST', 'k-GSST']; // Names of the series to remove

                        // Loop through chart series and remove specific ones
                        for (let i = chart.series.length - 1; i >= 0; i--) {
                            const seriesName = chart.series[i].name;
                            if (seriesToRemove.includes(seriesName)) {
                                chart.series[i].remove(false); // Remove series without redrawing
                            }
                        }

                        // Add the new series
                        newSeries.forEach(series => {
                            chart.addSeries(series, false); // Add series without redrawing\

                        });


                        chart.redraw(); // Finally redraw the chart
                    } else {
                        console.error(`No data found for selected material: ${selectedMaterial}`);
                    }
                });
            });

        });

    </script>
</body>

</html>
